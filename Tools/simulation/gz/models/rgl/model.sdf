          <frame>RGLLidar</frame>
          <pattern_preset>Alpha Prime</pattern_preset>
        </plugin>
      </sensor>
      <inertial>
        <inertia>
          <ixx>0.14580000000000001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.14580000000000001</iyy>
          <iyz>0</iyz>
          <izz>0.125</izz>
        </inertia>
        <mass>0.001</mass>
        <pose>0 0 0 0 -0 0</pose>
      </inertial>
      <collision name='lidar_collision'>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode/>
          </friction>
          <bounce/>
          <contact/>
