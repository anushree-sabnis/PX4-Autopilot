<?xml version="1.0" encoding="UTF-8"?>
<sdf version='1.9'>
  <model name='RGLLidar'>
    <pose>0 0 0 0 -0 0</pose>
    <link name='lidar_link'>
      <!-- Custom sensor with RGL plugin that simulates lidar -->
